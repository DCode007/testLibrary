<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="custom_font">
        <enum name="avenir_black" value="1"/>
        <enum name="avenir_book" value="2"/>
        <enum name="avenir_heavy" value="3"/>
        <enum name="avenir_light" value="4"/>
        <enum name="avenir_medium" value="5"/>
    </attr>
    <attr name="list_orientation">
        <enum name="vertical" value="0"/>
        <enum name="horizontal" value="1"/>
    </attr>
    <attr name="list_type">
        <enum name="list" value="0"/>
        <enum name="grid" value="1"/>
        <enum name="staggered" value="2"/>
    </attr>
    <declare-styleable name="CustomButton"><attr name="custom_font"/><attr format="dimension" name="btn_line_spacing"/><attr format="dimension" name="btn_letter_spacing"/><attr format="dimension" name="btn_line_height"/></declare-styleable>
    <declare-styleable name="CustomEditText"><attr name="custom_font"/><attr format="dimension" name="edt_txt_line_spacing"/><attr format="dimension" name="edt_txt_letter_spacing"/><attr format="dimension" name="edt_txt_line_height"/></declare-styleable>
    <declare-styleable name="CustomRecyclerView"><attr format="integer" name="gird_span"/><attr name="list_type"/><attr name="list_orientation"/></declare-styleable>
    <declare-styleable name="CustomTextView"><attr name="custom_font"/><attr format="dimension" name="line_spacing"/><attr format="dimension" name="letter_spacing"/><attr format="dimension" name="line_height"/><attr format="boolean" name="strike"/></declare-styleable>
    <string name="app_name">My LibraryDemo</string>
</resources>